<bg xmlns:xsi='http://www.w3.org/2001/XMLSchema-instance' xsi:schemaLocation='https://monkeymanboy.github.io/BSML-Docs/ https://raw.githubusercontent.com/monkeymanboy/BSML-Docs/gh-pages/BSMLSchema.xsd'>
	<vertical>
		<checkbox-setting text="Enable Bottach Point" value="EnableBp"  apply-on-change="true"></checkbox-setting>
		<increment-setting text="BP Factor" value="BpFactor" bind-value="true" increment="1" min="1" formatter="BpFactorFormatter" apply-on-change="true"></increment-setting>
    <horizontal pad-bottom='2'>
      <button text='Factor - x10' on-click='FactorMinus10'></button>
      <button text='Factor + x10' on-click='FactorPlus10'></button>
    </horizontal>
    <increment-setting text="BP Failure Threshold" value="BpFailureThreshold"  increment="0.1" min="0" formatter="BpFailureThresholdFormatter" apply-on-change="true"></increment-setting>
  </vertical>
</bg>